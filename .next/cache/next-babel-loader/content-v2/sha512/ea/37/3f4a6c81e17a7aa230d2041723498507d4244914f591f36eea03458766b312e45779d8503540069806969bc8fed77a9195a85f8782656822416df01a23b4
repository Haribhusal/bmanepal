\n  data[\"files\"] = [];\n\n  var form_data = new FormData();\n  for (var key in data) {\n    form_data.append(key, data[key]);\n  }\n  const config = {\n    headers: {\n      \"content-type\": \"multipart/form-data\",\n    },\n  };\n  axios.post(\"/api/proposal/request/\", form_data, config).then((res) => {\n    dispatch({\n      type: PROPOSAL_REQUEST,\n      payload: res.data,\n    });\n  });\n};"]},"metadata":{},"sourceType":"module"}